{
  "name": "form-generator",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build",
    "build:report": "vue-cli-service build --report",
    "build:render": "vue-cli-service build --target lib --name form-gen-render --dest ./src/components/render/lib/ ./src/components/render/render.js",
    "build:parser": "vue-cli-service build --target lib --name form-gen-parser --dest ./src/components/parser/lib/ ./src/components/parser/index.js",
    "build:tinymce": "vue-cli-service build --target lib --name form-gen-tinymce --dest ./src/components/tinymce/lib/ ./src/components/tinymce/index.js",
    "build:engine-components": "vue-cli-service build --target lib --name engine-components --dest ./src/engine-components/lib/ ./src/engine-components/index.js",
    "lint": "vue-cli-service lint",
    "dev": "vue-cli-service serve"
  },
  "dependencies": {
    "@babel/parser": "^7.7.4",
    "bcore": "0.0.22",
    "clipboard": "^2.0.4",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "lodash": "^4.17.11",
    "throttle-debounce": "^2.1.0",
    "vue": "^2.6.11",
    "vuedraggable": "^2.23.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@vue/cli-plugin-babel": "~4.3.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcore": "^0.0.22",
    "core-js": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-vue": "^6.2.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "inline-style-prefixer": "^2.0.5",
    "node-sass": "^4.14.1",
    "sass": "^1.23.7",
    "sass-loader": "^7.3.1",
    "svg-sprite-loader": "^4.1.6",
    "vue-color": "^2.7.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.30.0"
  },
  "homepage": "https://jakhuang.github.io/form-generator"
}
